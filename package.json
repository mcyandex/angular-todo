    "@angular/platform-browser": "^14.0.0",
    "@angular/platform-browser-dynamic": "^14.0.0",
    "@angular/router": "^14.0.0",
    "compression": "^1.7.4",
    "cookie-parser": "^1.4.6",
    "cookie-session": "^2.0.0",
    "csurf": "^1.11.0",
    "express": "^4.18.1",
    "helmet": "^6.0.0",
    "heroku-ssl-redirect": "^0.1.1",
    "knex": "^2.3.0",
    "remult": "^0.16.0",
    "rxjs": "~7.5.0",
